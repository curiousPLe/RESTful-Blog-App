<% include ./partials/header %>
<h1>INDEX PAGE!!!</h1>

<% blogposts.forEach(function(blog){ %>
    <div>
        <h2><%=blog.title%></h2>
        <img src="<%=blog.image%>">
        <div><%=blog.dateCreated%></div>
        <p><%=blog.body.substring(0,100) %></p>
        <a href="/blogs/<%=blog._id %>">Read More</a>
    </div>
<% }) %>

<% include ./partials/footer %>